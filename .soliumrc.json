{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "security/no-block-members": [
      "off"
    ],
    "no-experimental": [
      "off"
    ],
    "operator-whitespace": [
      "warning"
    ]
  }
}
