{
  "name": "truffle-template",
  "version": "1.0.0",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "npx truffle compile",
    "deploy:development": "npx truffle deploy --network development",
    "deploy:mainnet": "npx truffle deploy --network mainnet",
    "deploy:kovan": "npx truffle deploy --network kovan",
    "test": "scripts/test.sh",
    "coverage": "npx truffle run coverage",
    "lint:solhint": "npx solhint \"./contracts/**/*.sol\"",
    "lint:ethlint": "npx ethlint --dir contracts/",
    "lint:eslint": "eslint .",
    "lint": "npm run lint:solhint && npm run lint:ethlint && npm run lint:eslint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "@truffle/hdwallet-provider": "1.0.30",
    "core-js": "3.6.4",
    "dotenv": "8.2.0",
    "regenerator-runtime": "0.13.3",
    "truffle": "5.1.12",
    "truffle-flattener": "1.4.2",
    "truffle-plugin-verify": "0.3.8"
  },
  "devDependencies": {
    "@codechecks/client": "0.1.10",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eth-gas-reporter": "0.2.15",
    "ethlint": "1.2.5",
    "ganache-cli": "6.8.2",
    "solhint": "2.3.0",
    "solidity-coverage": "0.7.2"
  }
}
